{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "react",
      "lib": ["es6"],
      "moduleResolution": "node",
      "noEmit": true,
      "strict": true,
      "target": "esnext",
      "baseUrl": "./",
      "typeRoots": [
        "./js/types",
        "./node_modules/@types"
      ],
      "paths": {
        "src/*": ["js/*"],
        "res/*": ["js/res/*"],
        "types/*": ["js/types/*"]
      }
    },
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js"
    ]
  }